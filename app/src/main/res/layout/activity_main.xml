<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="pradipta.si3905.com.pradiptapriyaadyatmika_1202154221_studycase4.MainActivity">

    <Button
        android:id="@+id/btnList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/listNama"
        android:onClick="pindahList"
        android:layout_weight="1"
        android:layout_margin="5sp"/>

    <Button
        android:id="@+id/btnSrc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pencari Gambar"
        android:onClick="pindahSrc"
        android:layout_weight="1"
        android:layout_margin="5sp"/>
</LinearLayout>
